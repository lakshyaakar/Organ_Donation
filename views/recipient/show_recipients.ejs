<% include partials/header %>
<div style="width:100%; text-align:center; margin-top:50px; margin-left:25%;">
	<table border=1>
	<tr style="border:1px solid black;">
		<th style="padding:10px;">Name</th>
		<th style="padding:10px;">UserID</th>
		<th style="padding:10px;">Blood_Grp</th>
		<th style="padding:10px;">Organ required</th>
		<th style="padding:10px;">Contact number</th>
		<th style="padding:10px;">City</th>				
	</tr>
	<% recipients.forEach(function(recipient){ %>
		<tr>	
		<td style="padding: 10px;"><%= recipient.name %></td>
		<td style="padding: 10px;"><%= recipient.userID %></td>
		<td style="padding: 10px;"><%= recipient.Blood_Grp %></td>
		<td style="padding: 10px;"><%= recipient.Organ_needed %></td>
		<td style="padding: 10px;"><%= recipient.contact_no %></td>
		<td style="padding: 10px;"><%= recipient.city %></td>
		<tr>
	<% }); %>
	</table>
	</div>
<% include partials/footer %>
